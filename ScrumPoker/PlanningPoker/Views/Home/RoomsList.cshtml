@model IEnumerable<PlanningPoker.Models.RoomListModel>
@{
    ViewData["Title"] = "RoomsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="topicTable" align="center" style="text-align:center;">
    <tr>
        <th width="100px">
            Id
        </th>
        <th width="400px">
            Название
        </th>
        <th >
            Участники
        </th>
        <th>
            +
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Title
            </td>
            <td>
                @item.Count
            </td>

            <td>
                <a asp-action="RoomJoin" asp-route-id="@item.Id">Join</a>
            </td>

        </tr>
    }



</table>
