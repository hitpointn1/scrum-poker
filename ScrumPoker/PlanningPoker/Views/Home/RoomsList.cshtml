
@{
    ViewData["Title"] = "RoomsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table style="border-color: cadetblue;" cellpadding="5" cellspacing="10" border="1">
    <tr>
        <td>
            Id
        </td>
        <td>
            Название
        </td>
        <td>
            Участники
        </td>
        <td align="center">
            +
        </td>
    </tr>
    @for (int i = 0; i < 10; i++)
    {
        <tr>
            <td>
                @i
            </td>
            <td>
                Name
            </td>
            <td>
                @i/10
            </td>
            <td>
                <input type="button" value="Join" />
            </td>
        </tr>
    }


</table>
